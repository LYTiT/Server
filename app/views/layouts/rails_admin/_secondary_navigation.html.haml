%ul.nav.pull-right.root_links
  - actions(:root).each do |action|
    %li{class: "#{action.action_name}_root_link"}= link_to wording_for(:menu, action), { action: action.action_name, controller: 'rails_admin/main' }, class: "pjax"
  - if main_app_root_path = (main_app.root_path rescue false)
    %li= link_to t('admin.home.name').capitalize, main_app_root_path
    -#%li= link_to 'Download CSV Report by VOTE', "/admin"#VotesCsv.instance.csv_file.url
    -#%li= link_to 'Download CSV Report by VENUE', "/admin"#VenuesCsv.instance.csv_file.url
    %li
    =link_to '/sign_out', method: :get do
      %span
        =t('admin.misc.log_out')
      %small
        ="(#{current_user.email})"  
