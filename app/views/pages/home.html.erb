<div class="container-fluid splash_login">
  <div class="row">
    <div class="col-xs-1 col-sm-3 col-lg-3">&nbsp;</div>
    <div class="col-xs-10 col-sm-6 col-lg-6">
      <%= image_tag "splash_logo.png", class: "center-block"  %>
      <div class="center-block">
        <% if flash[:notice].present? %>
          <div class="alert alert-danger"><%= flash[:notice] %></div>
        <% end %>
        <div class="row downloads">
          <div class="col-sm-6">
            <center>
              <br />
              <a href="#">
                <%= image_tag "Download_on_the_App_Store_Badge_US-UK_135x40.svg", height: "50px", alt: "Download on the App Store" %>
              </a>
            </center>
          </div>
          <div class="col-sm-6">
            <center>
              <br />
              <%= image_tag "en_generic_rgb_wo_60.png", height: "50px", alt: "Get it on Google Play" %>
              <p style="margin-top: 5px;">Coming Soon!</p>
            </center>
          </div>
        </div>
      </div>
    </div>
    <div class="col-xs-1 col-sm-3 col-lg-3">&nbsp;</div>
  </div>
</div>
