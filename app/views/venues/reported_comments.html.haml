%table
  - @flagged_comments.each do |fc|
    %tr
      %td{ style: "border-bottom:solid 1px black; padding: 10px"}
        = fc.user.name
        reported
        = "(#{fc.message})"
        for comment
        = "(#{fc.venue_comment.comment})"
        on venue
        = "(#{fc.venue_comment.venue.try(:name)})"





